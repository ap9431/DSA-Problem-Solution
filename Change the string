string modify (string s)
   {
       if(s[0]>='a' && s[0]<='z'){
           
           for(int i=0;i<s.size();i++)
           {
               if(s[i]>='A' && s[i]<='Z'){
                   s[i]=s[i]+32;
               }
           }
           
       }
       
       if(s[0]>='A' && s[0]<='Z'){
           
           for(int i=0;i<s.size();i++){
               if(s[i]>='a' && s[i]<='z'){
                   s[i]=s[i]-32;
               }
           }
       }
       
       return s;
   }
